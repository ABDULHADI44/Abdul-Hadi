#Labtask1

str=input("Please Enter a string: ")
str2=''
stack1=[]
for i in str:
 stack1.append(i)
while len(stack1)!=0:
 str2=str2+stack1.pop()
if(str2==str):
 print("The string entered by you is palendrom")
else:
 print("The string entered by you is not a palendrom")




#labtask2

exp=input(" Please Enter an  expression:")
variable1=0
print(exp)
counter=0
for i in exp:
 if(i=='(' or i=='[' or i=='{'):
    print(i)
    counter=counter+1
 if(i==')' or i==']' or i=='}'):
   if(counter==0):
     print("Invalid expression")
     variable1=variable1+1
   else:
     counter=counter-1
if(counter==0 and variable1==0):
 print("Expression you entered is balanced")
else:
 print("Expression you entered is unbalanced")
 
 
 




